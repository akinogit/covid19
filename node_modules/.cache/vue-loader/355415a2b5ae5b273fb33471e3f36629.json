;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0HA;AACA;AACA","file":"Navbar.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n  <div>\r\n    <v-app-bar app color=\"#FB8C00\" dark fixed>\r\n      <h1>\r\n        <router-link to=\"/\" class=\"display-1 font-weight-thin white--text\"\r\n          >COVID-19</router-link\r\n        >\r\n      </h1>\r\n      <v-spacer></v-spacer>\r\n      <v-toolbar-items class=\"d-none d-sm-flex d-md-flex d-lg-flex\">\r\n        <v-btn text\r\n          ><router-link to=\"/\" class=\"title font-weight-light white--text\"\r\n            >Home</router-link\r\n          ></v-btn\r\n        >\r\n        <v-btn text\r\n          ><router-link to=\"/about\" class=\"title font-weight-light white--text\"\r\n            >About</router-link\r\n          ></v-btn\r\n        >\r\n        <v-menu offset-y>\r\n          <template v-slot:activator=\"{ on }\">\r\n            <v-btn class=\"mr-2\" text v-on=\"on\"\r\n              ><span class=\"title font-weight-light white--text\">Data</span\r\n              ><span class=\"mdi mdi-menu-down\"></span\r\n            ></v-btn>\r\n          </template>\r\n          <v-list>\r\n            <v-list-item-group v-model=\"model\">\r\n              <v-subheader>Data</v-subheader>\r\n              <v-list-item\r\n                v-for=\"(item, i) in items\"\r\n                :key=\"i\"\r\n                @click=\"click(i)\"\r\n              >\r\n                <v-list-item-icon>\r\n                  <v-icon>{{ item.icon }}</v-icon>\r\n                </v-list-item-icon>\r\n                <v-list-item-title v-text=\"item.text\"></v-list-item-title>\r\n              </v-list-item>\r\n            </v-list-item-group>\r\n          </v-list>\r\n        </v-menu>\r\n      </v-toolbar-items>\r\n      <v-app-bar-nav-icon\r\n        @click.stop=\"drawer = !drawer\"\r\n        class=\"d-flex d-sm-none\"\r\n        right\r\n      ></v-app-bar-nav-icon>\r\n    </v-app-bar>\r\n    <v-navigation-drawer v-model=\"drawer\" absolute temporary light>\r\n      <v-subheader>Menu</v-subheader>\r\n      <v-divider></v-divider>\r\n      <v-list dense>\r\n        <v-list-item @click=\"click('home')\">\r\n          <v-list-item-icon>\r\n            <v-icon>mdi-home</v-icon>\r\n          </v-list-item-icon>\r\n          <v-list-item-content>\r\n            <v-list-item-title>Home</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n        <v-list-item @click=\"click('about')\">\r\n          <v-list-item-icon>\r\n            <v-icon>mdi-web</v-icon>\r\n          </v-list-item-icon>\r\n          <v-list-item-content>\r\n            <v-list-item-title>About</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n        <v-list-item v-for=\"(item, i) in items\" :key=\"i\" @click=\"click(i)\">\r\n          <v-list-item-icon>\r\n            <v-icon>{{ item.icon }}</v-icon>\r\n          </v-list-item-icon>\r\n          <v-list-item-content>\r\n            <v-list-item-title>{{ item.text }}</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n      </v-list>\r\n    </v-navigation-drawer>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"Navbar\",\r\n  data: () => {\r\n    return {\r\n      items: [\r\n        {\r\n          icon: \"mdi-map-marker\",\r\n          text: \"感染状況マップ\"\r\n        },\r\n        {\r\n          icon: \"mdi-chart-bar\",\r\n          text: \"各種グラフ\"\r\n        }\r\n      ],\r\n      drawer: null,\r\n      model: 1\r\n    };\r\n  },\r\n  methods: {\r\n    click(i) {\r\n      if (i === 0) {\r\n        this.$router.push(\"/map\");\r\n      }\r\n      if (i === 1) {\r\n        this.$router.push(\"/graph\");\r\n      }\r\n      if (i === \"home\") {\r\n        // 現在のパスが'/'なら、エラーが出るため何もしないようにしたい\r\n        this.$router.push(\"/\");\r\n      }\r\n      if (i === \"about\") {\r\n        this.$router.push(\"/about\");\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\na {\r\n  text-decoration-line: none;\r\n}\r\n</style>\r\n"]}]}