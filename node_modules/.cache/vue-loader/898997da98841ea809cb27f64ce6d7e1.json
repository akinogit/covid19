s=\"mr-2\" text\n          ><router-link to=\"/\" class=\"title font-weight-light white--text\"\n            >Home</router-link\n          ></v-btn\n        >\n        <v-btn class=\"mr-2\" text\n          ><router-link to=\"/about\" class=\"title font-weight-light white--text\"\n            >About</router-link\n          ></v-btn\n        >\n        <v-menu offset-y>\n          <template v-slot:activator=\"{ on }\">\n            <v-btn class=\"mr-2\" text v-on=\"on\"\n              ><span class=\"title font-weight-light white--text\">Data</span\n              ><span class=\"mdi mdi-menu-down\"></span\n            ></v-btn>\n          </template>\n          <v-list>\n            <v-list-item-group v-model=\"model\">\n              <v-subheader>Data</v-subheader>\n              <v-list-item\n                v-for=\"(item, i) in items\"\n                :key=\"i\"\n                @click=\"click(i)\"\n              >\n                <v-list-item-icon>\n                  <v-icon>{{ item.icon }}</v-icon>\n                </v-list-item-icon>\n                <v-list-item-title v-text=\"item.text\"></v-list-item-title>\n              </v-list-item>\n            </v-list-item-group>\n          </v-list>\n        </v-menu>\n      </v-toolbar-items>\n      <v-app-bar-nav-icon\n        @click.stop=\"drawer = !drawer\"\n        class=\"d-flex d-sm-none\"\n        right\n      ></v-app-bar-nav-icon>\n    </v-app-bar>\n    <v-navigation-drawer v-model=\"drawer\" absolute temporary light>\n      <v-subheader>Menu</v-subheader>\n      <v-divider></v-divider>\n      <v-list dense>\n        <v-list-item @click=\"click('home')\">\n          <v-list-item-icon>\n            <v-icon>mdi-home</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title>Home</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n        <v-list-item @click=\"click('about')\">\n          <v-list-item-icon>\n            <v-icon>mdi-web</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title>About</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n        <v-list-item v-for=\"(item, i) in items\" :key=\"i\" @click=\"click(i)\">\n          <v-list-item-icon>\n            <v-icon>{{ item.icon }}</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title>{{ item.title}}</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"Navbar\",\n  data: () => {\n    return {\n      items: [\n        {\n          icon: \"mdi-map-marker\",\n          text: \"感染状況マップ\"\n        },\n        {\n          icon: \"mdi-chart-bar\",\n          text: \"各種グラフ\"\n        }\n      ],\n      drawer: null,\n      model: 1\n    };\n  },\n  methods: {\n    click(i) {\n      if (i === 0) {\n        this.$router.push(\"/map\");\n      }\n      if (i === 1) {\n        this.$router.push(\"/graph\");\n      }\n      if (i === \"home\") {\n        // 現在のパスが'/'なら、エラーが出るため何もしないようにしたい\n        this.$router.push(\"/\");\n      }\n      if (i === \"about\") {\n        this.$router.push(\"/about\");\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\na {\n  text-decoration-line: none;\n}\n</style>\n"]}]}